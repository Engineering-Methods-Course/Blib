<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="432.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="gui.ServerMonitorFrameController">
    <children>
        <VBox prefHeight="370.0" prefWidth="600.0" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="50.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <children>
                <TableView fx:id="serverMonitorTable" VBox.vgrow="ALWAYS">
                    <columns>
                        <TableColumn fx:id="clientNum" prefWidth="66.0" text="#"/>
                        <TableColumn fx:id="ip" prefWidth="167.0" text="IP"/>
                        <TableColumn fx:id="host" prefWidth="221.0" text="Host"/>
                        <TableColumn fx:id="status" prefWidth="145.0" text="Status"/>
                    </columns>
                </TableView>
            </children>
        </VBox>
        <StackPane prefHeight="50.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                   AnchorPane.rightAnchor="0.0">
            <children>
                <Button id="btnExit" fx:id="btnExit" layoutX="256.0" layoutY="381.0" mnemonicParsing="false"
                        onAction="#getExitBtn" prefHeight="37.0" prefWidth="67.0" text="Exit"/>
            </children>
        </StackPane>
    </children>
</AnchorPane>